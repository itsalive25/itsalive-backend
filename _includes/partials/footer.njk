 <script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"></script>

<script>
  async function loadPad(targetId, padUrl) {
    try {
      const res = await fetch(padUrl);
      const markdown = await res.text();
      const html = marked.parse(markdown);

      const target = document.getElementById(targetId);
      if (target) {
        target.innerHTML = html;
      } else {
        console.warn(`Element with id "${targetId}" not found.`);
      }
    } catch (err) {
      console.error(`Fehler beim Laden von ${padUrl} â†’`, err);
    }
  }

  document.addEventListener("DOMContentLoaded", () => {
    loadPad("submissions-01", "https://pad.piratenpartei.at/p/ITSA-assignment-01/export/txt");
    loadPad("submissions-00", "https://pad.piratenpartei.at/p/ITSA-assignment-00/export/txt");
    loadPad("material", "https://pad.piratenpartei.at/p/ITSA-material/export/txt");
  });
</script>




  
